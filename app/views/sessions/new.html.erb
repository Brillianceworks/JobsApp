<h1>Sign In</h1>
<%= form_for(:sessions, :url => sessions_path) do |f| %>
  <div>
    <%= f.label :email %><br />
    <%= f.text_field :email %><br /><br />
  </div>
  <div>
    <%= f.label :password %><br />
    <%= f.password_field :password %><br /><br />
  </div>
  <div>
    <%= f.submit "Sign in" %>
  </div>
  
<% end %>
<p>New User? <%= link_to "Sign Up", signup_path %> </p>
